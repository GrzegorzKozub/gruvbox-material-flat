{
  "tokenColors": [
    {
      "name": "json names",
      "scope": ["meta.mapping.key.json string.quoted.double.json"],
      "settings": { "foreground": "{{blue}}" }
    },
    {
      "name": "toml names",
      "scope": [
        "entity.name.table.toml",
        "entity.name.tag.toml",
        "meta.tag.key.toml",
        "meta.tag.table.toml"
      ],
      "settings": { "foreground": "{{blue}}" }
    },
    {
      "name": "toml strings",
      "scope": [
        "string.quoted.double.basic.toml",
        "string.quoted.single.literal.toml"
      ],
      "settings": { "foreground": "{{green}}" }
    },
    {
      "name": "man headings",
      "scope": [
        "markup.heading.commands.man",
        "markup.heading.env.man",
        "markup.heading.other.man",
        "markup.heading.synopsis.man"
      ],
      "settings": { "foreground": "{{yellow}}" }
    },
    {
      "name": "man command line options",
      "scope": ["entity.name.command-line-option.man"],
      "settings": { "foreground": "{{green}}" }
    },
    {
      "name": "man parameters",
      "scope": ["variable.parameter.man"],
      "settings": { "foreground": "{{red}}" }
    }
  ]
}
